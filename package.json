{
  "name": "vs-tictactoe",
  "publisher": "timthedev07",
  "galleryBanner": {
    "color": "#67f00b"
  },
  "repository": {
    "url": "https://github.com/timthedev07/vs-tictactoe"
  },
  "icon": "media/vs-tictactoe.png",
  "author": {
    "email": "timbusiness.work07@gmail.com",
    "name": "Tim"
  },
  "displayName": "vs-tictactoe",
  "description": "Play tic tac toe right in your VScode sidebar while procrastinating(I know you often do).",
  "version": "0.0.1",
  "license": "MIT",
  "engines": {
    "vscode": "^1.61.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:vs-tictactoe.play",
    "onView:vs-tictactoe.side-bar"
  ],
  "main": "./dist/extension.js",
  "contributes": {
    "viewsContainers": {
      "activitybar": [
        {
          "id": "vs-tictactoe-sidebar-view",
          "title": "VS-TicTacToe",
          "icon": "media/ttt.svg"
        }
      ]
    },
    "views": {
      "vs-tictactoe-sidebar-view": [
        {
          "type": "webview",
          "id": "vs-tictactoe.side-bar",
          "name": "VS-TicTacToe",
          "icon": "media/ttt.svg",
          "contextualTitle": "VS-TicTacToe"
        }
      ]
    },
    "commands": [
      {
        "command": "vs-tictactoe.play",
        "title": "Start a tic tac toe game."
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "yarn run package",
    "compile": "webpack",
    "watch": "concurrently \"rollup -c -w\" \"webpack --watch\"",
    "package": "webpack --mode production --devtool hidden-source-map",
    "compile-tests": "tsc -p . --outDir out",
    "watch-tests": "tsc -p -w . --outDir out",
    "pretest": "yarn run compile-tests && yarn run compile && yarn run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^21.0.1",
    "@rollup/plugin-node-resolve": "^13.0.6",
    "@rollup/plugin-typescript": "^8.3.0",
    "@tsconfig/svelte": "^2.0.1",
    "@types/glob": "^7.1.4",
    "@types/mocha": "^9.0.0",
    "@types/node": "14.x",
    "@types/vscode": "^1.61.0",
    "@typescript-eslint/eslint-plugin": "^4.31.1",
    "@typescript-eslint/parser": "^4.31.1",
    "@vscode/test-electron": "^1.6.2",
    "eslint": "^7.32.0",
    "glob": "^7.1.7",
    "mocha": "^9.1.1",
    "rollup": "^2.58.0",
    "rollup-plugin-css-only": "^3.1.0",
    "rollup-plugin-svelte": "^7.1.0",
    "rollup-plugin-terser": "^7.0.2",
    "svelte-preprocess": "^4.9.8",
    "ts-loader": "^9.2.5",
    "typescript": "^4.4.3",
    "webpack": "^5.52.1",
    "webpack-cli": "^4.8.0"
  },
  "dependencies": {
    "svelte": "^3.44.0",
    "svelte-check": "^2.2.7"
  }
}
